<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byType">
    <!--这个default-autowire作者测试的时候并不会影响到测试结果，我们后面分析-->
    <bean id = "user" class="config.ignore.User"></bean>
    <bean id = "role" class="config.ignore.Role"></bean>
    <bean id = "admin" class="config.ignore.Admin"></bean>
    <bean id="person" class="config.ignore.Person"></bean>
</beans>